!-------------------------------------------------------------------------------
!
!	U R X V T
!
! General
!urxvt*geometry:         80x26
urxvt.termName:  rxvt-256color
!urxvt.loginShell:         true
urxvt.scrollBar:         false
!urxvt.internalBorder:        6
!urxvt.borderWidth:           6
!urxvt.secondaryScroll:    true
!URxvt*borderLess: false
urxvt.saveLines:         65535
!urxvt.cursorBlink:        true
!! Notifications for pine and irssi under awesome
urxvt.urgentOnBell:       true
!!
!! Extensions
!!urxvt*perl-lib:         /usr/lib/urxvt/perl/
!!urxvt*perl-ext-common:  default,matcher,selection,mark-yank-urls,tabbed
urxvt*perl-ext-common:  default,matcher
!
!!urxvt*urlLauncher:      /usr/local/bin/firefox
!!urxvt*matcher.button:   1
!! So we catch even nasty URLs
!!urxvt.cutchars:         `()'*<>[]{|}"
!!URxvt*cutchars:         `"()'*<>[]{|}
!!! on active les modules nécessaires
URxvt.perl-ext: selection
!,mark-yank-urls
!!!! on définit comment passer en mode "copie d'url": ici il s'agit de alt-u 
!URxvt.keysym.M-u: perl:mark-yank-urls:activate_mark_mode
!!!! on peut choisir de souligner les urls
!URxvt.underlineURLs: true
!!!! on définit ici le navigateur par défaut (pour lancer une url sélectionnée
!!avec entrée)
URxvt.urlLauncher: firefox
!URxvt.perl-ext:        default,matcher,keyboard-select
!URxvt.urlLauncher:     /usr/bin/firefox
! permet de cliquer sur les urls avec la souris
URxvt.matcher.button:  1 
!URxvt.keysym.M-Escape:  perl:keyboard-select:activate
!URxvt.keysym.M-s:       perl:keyboard-select:search

!
! Appearance
!urxvt.font:     -*-terminus-medium-r-*-*-12-120-*-*-*-*-*-*
!urxvt.boldFont: -*-terminus-bold-*-*-*-12-*-*-*-*-*-*-*
urxvt.font:     xft:Inconsolata:size=18:antialias=true,xft:Anonymous Pro:size=15
!urxvt.font:     xft:Liberation Mono:size=14
!urxvt.font:     xft:Inconsolata Medium:size=9
!urxvt.font:     xft:Anonymous Pro:size=14
!urxvt.boldFont: xft:terminus:style=Bold:size=14
!URxvt.tabbed.tabbar-fg:      7
!URxvt.tabbed.tabbar-bg:      4
!URxvt.tabbed.tab-fg:         0
!URxvt.tabbed.tab-bg:         6
!URxvt.keysym.C-1: command:\033]50;xft:Liberation Mono-10\007
!URxvt.keysym.C-2: command:\033]50;xft:Liberation Mono-12\007
!URxvt.keysym.C-3: command:\033]50;xft:Liberation Mono-14\007
!URxvt.keysym.C-4: command:\033]50;xft:Liberation Mono-20\007
!!
!urxvt.shading:     30
!urxvt.cursorColor: #FFFFFF
!! Pseudo Transparency
!urxvt.transparent: false
!! Real transparency (with composite and xcompmgr)
!urxvtTrans*depth:      32
!urxvt*depth:      32
!!urxvtTrans*background: rgba:0000/0000/0000/dddd
!urxvtTrans*tintColor:  grey
!URxvt.transparent:   true
!URxvt.shading:       20
! 
