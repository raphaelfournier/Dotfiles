! xscreensaver ---------------------------------------------------------------

!font settings
xscreensaver.Dialog.headingFont:        xft:Inconsolata Nerd Font:style=Medium:size=18
xscreensaver.Dialog.bodyFont:           xft:Inconsolata Nerd Font:style=Medium:size=18
xscreensaver.Dialog.labelFont:          xft:Inconsolata Nerd Font:style=Medium:size=18
xscreensaver.Dialog.unameFont:          xft:Inconsolata Nerd Font:style=Medium:size=18
xscreensaver.Dialog.buttonFont:         xft:Inconsolata Nerd Font:style=Medium:size=18
xscreensaver.Dialog.dateFont:           xft:Inconsolata Nerd Font:style=Medium:size=18
xscreensaver.passwd.passwdFont:         xft:Inconsolata Nerd Font:style=Medium:size=18
!general dialog box (affects main hostname, username, password text)
xscreensaver.Dialog.foreground:         #383a42
xscreensaver.Dialog.background:         #fafafa
xscreensaver.Dialog.topShadowColor:     #a0a1a7
xscreensaver.Dialog.bottomShadowColor:  #a0a1a7
xscreensaver.Dialog.Button.foreground:  #fafafa
xscreensaver.Dialog.Button.background:  #0184bc
!username/password input box and date text colour
xscreensaver.Dialog.text.foreground:    #ca1243
xscreensaver.Dialog.text.background:    #a0a1a7
xscreensaver.Dialog.internalBorderWidth:30
xscreensaver.Dialog.borderWidth:        2
xscreensaver.Dialog.shadowThickness:    4
!timeout bar (background is actually determined by Dialog.text.background)
xscreensaver.passwd.thermometer.foreground:  #4078f2
xscreensaver.passwd.thermometer.background:  #a0a1a7
xscreensaver.passwd.thermometer.width:       18
!datestamp format--see the strftime(3) manual page for details
xscreensaver.dateFormat:    
!%H:%M %d-%m-%Y
! /home/raph/.base16-manager/chriskempson/base16-shell/scripts/base16-one-light.sh
